{
  "name": "edify",
  "version": "1.0.0",
  "description": "An easy to use CMS library",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomdaniel-it/Edify.git"
  },
  "author": "Tom Daniel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomdaniel-it/Edify/issues"
  },
  "homepage": "/edify/",
  "dependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "^7.1.2",
    "axios": "^0.19.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-scripts": "3.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "typescript": "3.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:backend": "php -S 0.0.0.0:8000",
    "start:demo": "cd ../demo/ && php -S 0.0.0.0:8000",
    "build": "react-scripts build",
    "build:demo": "npm run build && mkdir -p ../demo/edify/backend/db/json/db && mv ../demo/edify/backend/db/json/db/ ../demo/edify-tmp/ && rm -rf ../demo/edify/* && cp -a build/. ../demo/edify/ && cp backend/ ../demo/edify/backend/ -r && mkdir -p ../demo/edify/backend/db/json/ && rm ../demo/edify/backend/db/json/db -rf && mv ../demo/edify-tmp ../demo/edify/backend/db/json/db",
    "build:demo:reset": "rm -rf ../demo/edify/backend/db/json/db/* && npm run build:demo",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .tsx"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0"
  }
}
